#include<iostream>
using namespace std;
#define N 3 
#define N1 9
int main()
{
    int i,j, a[N]={3,4,1};
    
    for(i=1;i<(N1+1);i++){
        printf("\n");
         for(j=1;j<(N1+1);j++){
            printf(" %d * %d = %d \t" ,i,j,i*j);
        
    }
    }
    
   
    cout<<"Hello world";
    
    
    return 0;
}
